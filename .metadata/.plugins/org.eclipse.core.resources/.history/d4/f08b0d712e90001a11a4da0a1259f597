package jdbc.vo;

public class Customer {
	private String id;
	private String pass;
	private String name; //컬럼명과 다름
	private String address;
	
	public Customer() {}
	public Customer(String id, String pass, String name, String address) {
		super();
		this.id = id;
		this.pass = pass;
		this.name = name;
		this.address = address;
	}
	
	
	

}

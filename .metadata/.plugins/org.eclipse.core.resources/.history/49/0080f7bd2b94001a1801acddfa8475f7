package at01;

public class AT01 {
	static long solution(int a, int b) {
		long answer = Math.min(a, b);
		int i;
		if(a==b) answer = a;
		if(a>b) {
			while(i == a-b) { 
				answer += a++;
				i++;
			}
		}
		if(a<b) {
			for(int i=a+1;i<b-a;i++) answer += i;
		}
		return answer;
	}

	public static void main(String[] args) {
		System.out.println(solution(3,5));

	}

}


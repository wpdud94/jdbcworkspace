package at01;

import javax.swing.plaf.synth.SynthSeparatorUI;

public class AT01 {
	

	public static void main(String[] args) {
		System.out.println(function.solution(6,4));

	}

}

class function {
	public static long solution(int a, int b) {
		long answer;
		int i = 0;
		if(a==b) answer = a;
		if(a>b) {
			while(i == a-b) {
				System.out.println("---");
				answer = answer + a;
				i++;
			}
		}
		if(a<b) {
			while(i == b-a) {
				b++;
				answer += b;
				i++;
			}
		}
		return answer;
	}
}
